        android:id="@+id/commentary5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/ViewDayMood6"
        app:layout_constraintEnd_toEndOf="@+id/ViewDayMood5"
        app:layout_constraintTop_toTopOf="@+id/ViewDayMood5"
        app:srcCompat="@android:drawable/sym_action_chat" />

    <ImageView
        android:id="@+id/commentary6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/ViewDayMood6"
        app:layout_constraintEnd_toEndOf="@+id/ViewDayMood6"
        app:layout_constraintTop_toTopOf="@+id/ViewDayMood6"
        app:srcCompat="@android:drawable/sym_action_chat" />

    <ImageView
        android:id="@+id/commentary7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/ViewDayMood7"
        app:layout_constraintEnd_toEndOf="@+id/ViewDayMood7"
        app:layout_constraintTop_toTopOf="@+id/ViewDayMood7"
        app:srcCompat="@android:drawable/sym_action_chat" />


</androidx.constraintlayout.widget.ConstraintLayout>