        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="Il y a quatre jours"
        app:layout_constraintStart_toStartOf="@+id/ViewDayMood4"
        app:layout_constraintTop_toTopOf="@+id/ViewDayMood4" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="Il y a cinq jours"
        app:layout_constraintStart_toStartOf="@+id/ViewDayMood5"
        app:layout_constraintTop_toTopOf="@+id/ViewDayMood5" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="Il y a six jours"
        app:layout_constraintStart_toStartOf="@+id/ViewDayMood6"
        app:layout_constraintTop_toTopOf="@+id/ViewDayMood6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="Il y a une semaine"
        app:layout_constraintStart_toStartOf="@+id/ViewDayMood7"
        app:layout_constraintTop_toTopOf="@+id/ViewDayMood7" />

    <ImageView
        android:id="@+id/commentary1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/mood_day1"
        app:layout_constraintEnd_toEndOf="@+id/mood_day1"
        app:layout_constraintTop_toTopOf="@+id/mood_day1"
        app:srcCompat="@android:drawable/sym_action_chat" />

    <ImageView
        android:id="@+id/commentary2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/ViewDayMood3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ViewDayMood2"
        app:srcCompat="@android:drawable/sym_action_chat" />

    <ImageView
        android:id="@+id/commentary3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
